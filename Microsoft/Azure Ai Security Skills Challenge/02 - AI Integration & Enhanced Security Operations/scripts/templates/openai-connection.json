{
    "location": "{location}",
    "properties": {
        "displayName": "Azure OpenAI Service",
        "api": {
            "id": "/subscriptions/{subscriptionId}/providers/Microsoft.Web/locations/{location}/managedApis/azureopenai"
        },
        "authenticatedUser": {},
        "parameterValueSet": {
            "name": "keyBasedAuth",
            "values": {
                "azureOpenAIResourceName": {
                    "value": "{openAIServiceName}"
                },
                "azureOpenAIApiKey": {
                    "value": "{openAIApiKey}"
                }
            }
        }
    }
}
