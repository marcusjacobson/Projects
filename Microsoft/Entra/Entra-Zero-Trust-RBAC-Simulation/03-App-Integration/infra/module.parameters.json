{
    "global": {
        "location": "US"
    },
    "Configure-AppConsentGovernance": {
        "restrictAppRegistration": "true",
        "blockUserConsent": "true",
        "notifyReviewersExpirationInDays": "7",
        "enableAdminConsentRequests": "true",
        "blockUserConsentForRiskyApps": "true"
    },
    "Deploy-ReportingServicePrincipal": {
        "appName": "APP-Reporting-Automation",
        "certName": "EntraSimulationCert",
        "certValidityYears": "1",
        "keyCredentialDisplayName": "Automation Cert",
        "permissions": ["AuditLog.Read.All"]
    },
    "Remove-AppIntegration": {
    }
}
